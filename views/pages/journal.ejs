<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/header.ejs') %>
            <main class="main">
                <div class="container">
                    <section class="journal super-box">
                        <h2 class="journal__title title super-box__left"><%= title %></h2>
                        <div class="journal__inner super-box__right">
                            <% for (post of posts) { %>
                                <article class="post">
                                    <header class="post__header">
                                        <h3 class="post__title"><%= post.title %></h3>
                                        <span class="post__date"><%= post.convertedDate %></span>
                                    </header>
                                    <p class="post__text">
                                    <%= post.text %>
                                    </p>
                                </article>
                            <% } %>
                        </div>
                    </section>
                </div>
            </main>
            <%- include('../includes/footer.ejs') %>